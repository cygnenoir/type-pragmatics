	~t == table(~al, ~rt)
	~tresult == table(amany(A2, aone(A1)), 
 		tmany(rmany(y2, rone(x1)), tmany(rmany(y1, rone(x2)), tempty)))
	===================================================================================== test-projection3
	getSome(projection(amany(A2, aone(A1)), ~t)) == ~tresult
}

local {
	constructors 
		x1 : FVal
		x2 : FVal
		y1 : FVal
		y2 : FVal
		z1 : FVal
		z2 : FVal
		
	constructors
		TN : TName
		A1 : AName
		A2 : AName
		A3 : AName
		
	axiom
	~al == amany(A1, amany(A2, aone(A3)))
	~rt == tmany(rmany(x1, rmany(y2, rone(z2))), 
		tmany(rmany(x2, rmany(y1, rone(z1))), tempty))
	~t == table(~al, ~rt)
	~tresult == table(amany(A2, aone(A1)), 
 		tmany(rmany(y2, rone(x1)), tmany(rmany(y1, rone(x2)), tempty)))
	===================================================================================== test-projection3
	getSome(projection(amany(A2, aone(A1)), ~t)) == ~tresult

	goal
	~al == amany(A1, amany(A2, aone(A3)))
	~rt == tmany(rmany(x1, rmany(y2, rone(z2))), 
		tmany(rmany(x2, rmany(y1, rone(z1))), tempty))
	~t == table(~al, ~rt)
	~ts == bind(TN, ~t, empty)
	~q == SelectFrom1(amany(A2, aone(A1)), TN)
	~tresult == table(amany(A2, aone(A1)), 
		tmany(rmany(y2, rone(x1)), tmany(rmany(y1, rone(x2)), tempty)))
	=================================================================================== test-selectfrom1-2
	reduce(conf(~q, ~ts)) == some(conf(TValue(~tresult), ~ts))
}

